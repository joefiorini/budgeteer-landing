{
  "name": "budgeteer-landing",
  "version": "1.0.0",
  "description": "Landing page for budgeteer app",
  "main": "index.js",
  "bin": "./bin/budgeteer-landing",
  "files": [
    "bin",
    "webpack-stats.json"
  ],
  "scripts": {
    "prestart": "npm prune",
    "pkg": "npm run build:backend && npm run build:frontend && npm pack",
    "clean": "rm -rf dist/",
    "start": "nodemon --exec npm run start:dev",
    "start:dev": "npm install && concurrently --kill-others \"npm run build-server\" \"npm run dev-server\"",
    "start:prod": "node server/boot.js",
    "build:backend": "webpack --config server/webpack.config.server.babel.js",
    "build:frontend": "BABEL_ENV=frontend $(npm bin)/webpack --config server/webpack.config.babel.js",
    "build-server": "BABEL_ENV=frontend node dev/webpack.boot.js",
    "dev-server": "BABEL_ENV=backend node dev/boot.js",
    "test": "eslint {src,dev,server}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joefiorini/budgeteer-landing.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joefiorini/budgeteer-landing/issues"
  },
  "homepage": "https://github.com/joefiorini/budgeteer-landing#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "git+https://github.com/iominh/extract-text-webpack-plugin.git",
    "html-webpack-plugin": "^2.17.0",
    "lost": "^6.8.0",
    "nodemon": "^1.9.2",
    "null-loader": "^0.1.1",
    "postcss-autoreset": "^1.1.5",
    "postcss-cssnext": "^2.6.0",
    "postcss-initial": "^1.5.1",
    "postcss-loader": "^0.9.1",
    "postcss-modules-values": "^1.1.3",
    "postcss-pxtorem": "^3.3.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "svg-loader": "0.0.2",
    "svg-react-loader": "^0.3.3",
    "svgo": "^0.6.6",
    "svgo-loader": "^1.1.0",
    "wait-on": "^1.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "classnames": "^2.2.5",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "findup": "^0.1.5",
    "lost": "^6.8.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.5.1",
    "source-map-support": "^0.4.0"
  }
}
