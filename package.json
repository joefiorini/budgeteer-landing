{
  "name": "budgeteer-landing",
  "version": "1.0.0",
  "description": "Landing page for budgeteer app",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm prune",
    "build": "npm run build:clean && BABEL_ENV=frontend $(npm bin)/webpack --config dev/webpack.config.babel.js",
    "start": "nodemon --exec npm run start:dev",
    "start:dev": "npm install && concurrently --kill-others \"npm run build-server\" \"npm run dev-server\"",
    "start:prod": "node server/boot.js",
    "build:clean": "rm -rf dist/",
    "build-server": "BABEL_ENV=frontend node dev/webpack.boot.js",
    "dev-server": "BABEL_ENV=backend node dev/boot.js",
    "test": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joefiorini/budgeteer-landing.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joefiorini/budgeteer-landing/issues"
  },
  "homepage": "https://github.com/joefiorini/budgeteer-landing#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-webpack-loaders": "^0.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "exports-loader": "^0.6.3",
    "html-webpack-plugin": "^2.17.0",
    "lost": "^6.8.0",
    "nodemon": "^1.9.2",
    "postcss-autoreset": "^1.1.5",
    "postcss-cssnext": "^2.5.2",
    "postcss-initial": "^1.5.1",
    "postcss-loader": "^0.9.1",
    "postcss-pxtorem": "^3.3.1",
    "react-hot-loader": "^3.0.0-beta.1",
    "style-loader": "^0.13.1",
    "svg-loader": "0.0.2",
    "svg-react-loader": "^0.3.3",
    "svgo-loader": "^1.1.0",
    "wait-on": "^1.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "dotenv": "^2.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0"
  }
}
